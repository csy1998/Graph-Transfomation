	tile_0 -> trivial [label=is]
	tile_1 -> tile_2 [label=left_of]
	tile_1 -> trivial [label=is]
	tile_2 -> goal [label=is]
	agent -> tile_1 [label=on]
	box -> tile_2 [label=on]
}
